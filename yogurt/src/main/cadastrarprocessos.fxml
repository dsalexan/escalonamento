<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefHeight="299.0" prefWidth="429.0" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.CadastrarprocessosController">
   <children>
      <Label layoutX="10.0" layoutY="53.0" text="Tempo de chegada :" />
      <Label layoutX="10.0" layoutY="86.0" text="Tempo para processar :" />
      <Label layoutX="10.0" layoutY="119.0" text="Prioridade :" />
      <TextField fx:id="txtTempoChegada" layoutX="140.0" layoutY="49.0" prefHeight="25.0" prefWidth="39.0" />
      <TextField fx:id="txtTempoProcessar" layoutX="140.0" layoutY="82.0" prefHeight="25.0" prefWidth="39.0" />
      <TextField fx:id="txtPrioridade" layoutX="140.0" layoutY="115.0" prefHeight="25.0" prefWidth="39.0" />
      <ColorPicker fx:id="cbCor" layoutX="200.0" layoutY="82.0" />
      <Label layoutX="202.0" layoutY="53.0" text="Cor de Identificação :" />
      <Button fx:id="btnCriar" layoutX="13.0" layoutY="150.0" mnemonicParsing="false" onAction="#clickCriarProcesso" prefHeight="25.0" prefWidth="97.0" text="Criar Processo" />
      <Button fx:id="btnRemover" disable="true" layoutX="233.0" layoutY="150.0" mnemonicParsing="false" onAction="#clickRemover" text="Remover Processo" />
      <Button fx:id="btnProcessar" layoutX="197.0" layoutY="248.0" mnemonicParsing="false" onAction="#clickProcessar" text="Processar" />
      <ComboBox fx:id="cbAlgoritmo" layoutX="17.0" layoutY="213.0" onAction="#escolhaComboAlgoritmo" prefHeight="25.0" prefWidth="156.0" />
      <Label layoutX="16.0" layoutY="189.0" text="Algoritmo de Escalonamento:" />
      <Button fx:id="btnLimparFila" layoutX="270.0" layoutY="248.0" mnemonicParsing="false" onAction="#clickLimparFila" text="Limpar Fila de Processos" />
      <Label layoutX="16.0" layoutY="252.0" text="Quantum :" />
      <TextField fx:id="txtQuantum" layoutX="75.0" layoutY="248.0" prefHeight="25.0" prefWidth="39.0" />
      <Button fx:id="btnAtualizar" disable="true" layoutX="115.0" layoutY="150.0" mnemonicParsing="false" onAction="#clickAtualizar" prefHeight="25.0" prefWidth="114.0" text="Atualizar Processo" />
      <Button fx:id="btnLimpar" layoutX="350.0" layoutY="150.0" mnemonicParsing="false" onAction="#clickLimpar" prefHeight="25.0" prefWidth="67.0" text="Limpar" />
      <Label layoutX="10.0" layoutY="20.0" text="Id do Processo:" />
      <TextField fx:id="txtId" layoutX="112.0" layoutY="16.0" prefHeight="25.0" prefWidth="67.0" />
   </children>
</fx:root>
